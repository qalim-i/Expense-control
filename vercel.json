{
    "builds": [
        {
        "src": "expense_control/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" , "runtime": "python3.12.10" }
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "expense_control/wsgi.py"
        }
    ]
}